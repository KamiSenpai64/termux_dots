/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @background;
    text-color:             @foreground;
	font:				 	"Comfortaa 12";
}

window {
    width:      			225px;
    padding:    			25px;
    border:					0px;
    border-radius:          20px;
    border-color:           @accent;
    location:               center;
    x-offset:               0px;
    y-offset:               0px;
}

entry {
    expand: 				true;
    text-color:				@accent;
}
